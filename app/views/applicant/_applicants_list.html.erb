<table id="applicants2" class="display">
  <thead>
  <tr>
    <th>Name</th>
    <th>Roll No</th>
    <th>Gender</th>
    <th>Email Address</th>
    <th>Qualification</th>
    <th>Specification</th>
    <th>Percentage</th>
    <th>Score</th>
    <% if @pool == true %>
        <th class="table-header-repeat line-left"><a>College</a></th>
    <% end %>
  </tr>
  </thead>

  <tbody>
  <% @applicant.each do |app| %>

      <tr>
        <td><%= app.Name %></td>
        <td><%= app.RollNo %></td>
        <td><%= app.Gender %> </td>
        <td><%= app.EmailAdd %> </td>
        <td><%= app.Qualification %></td>
        <td><%= app.Branch %> </td>
        <td><%= app.Percentage %></td>
        <td><%= text_field :applicant, :score, :value => app.Score, :id => "#{app.id}", :onchange => "autoSave(this)" %></td>
        <% if @pool == true %>
            <td style="text-align: left;"><%= College.find_by_id(app.collegeId).name %> </td>
        <% end %>
      </tr>

  <% end %>
  </tbody>

</table>
<%= paginate @applicant %>
<br>